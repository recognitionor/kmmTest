CREATE TABLE IF NOT EXISTS testEntity (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL
);

getTestList:
SELECT *
FROM testEntity
ORDER BY id ASC;

insertTestEntity:
INSERT OR REPLACE
INTO testEntity(
    name,
    count,
    max
) VALUES (?, ?, ?);

deleteTest:
DELETE FROM testEntity
WHERE id = :id;

insertPlayer:
INSERT OR REPLACE
INTO Player(
    id,
    position
) VALUES (?, ?);

getPlayerList:
SELECT *
FROM Player
ORDER BY id ASC;
